{
  "name": "songlink",
  "version": "1.0.0",
  "description": "share music to anyone on any platform",
  "main": "server.js",
  "author": "juvenile-orca",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/juvenile-orca/songlink"
  },
  "engines": {
    "node": "5.0.0"
  },
  "scripts": {
    "dbCrawl": "node dbCrawl/dbCrawl.js",
    "clean": "rm -rf dist && mkdir dist && mkdir dist/scripts && mkdir dist/assets",
    "js:copy": "cp -a server/templates/linkTemplate/scripts/. dist/scripts/",
    "js:uglify": "uglifyjs server/templates/linkTemplate/scripts/listenerTools.js -o dist/scripts/listenerTools.js",
    "js:webpack": "NODE_ENV=production webpack --config webpack.production.config.js",
    "images:copy": "cp -a server/static/assets/. dist/assets/",
    "css:sass:prod": "node-sass sass/main.scss dist/main.css --output-style compressed",
    "css:sass:dev": "node-sass sass/main.scss dist/main.css",
    "css:sass:watch": "nodemon -e scss --watch ./sass -x \"npm run css:sass:dev\"",
    "serve": "npm run clean && npm run js:copy && npm run images:copy && npm run css:sass:dev",
    "postserve": "npm run css:sass:watch & nodemon --debug server/server.js --ignore components --ignore containers --ignore sass",
    "compile": "npm run clean && npm run js:uglify && npm run images:copy && npm run css:sass:prod && npm run js:webpack",
    "start": "node server/server.js",
    "test": "mocha test/models test/controllers test/server"
  },
  "dependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.1",
    "chai": "^3.4.1",
    "classnames": "^2.2.1",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "express": "^4.13.3",
    "history": "^1.13.1",
    "jquery": "^2.1.4",
    "mocha": "^2.3.4",
    "mongo": "^0.1.0",
    "mongoose": "^4.2.9",
    "mustache": "^2.2.0",
    "node-sass": "^3.4.2",
    "playmusic": "^2.1.1",
    "react": "^0.14.3",
    "react-clipboard.js": "^0.1.5",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "react-social": "^1.0.4",
    "react-tooltip": "^0.9.5",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0",
    "request": "^2.67.0",
    "shelljs": "^0.5.3",
    "supertest": "^1.1.0",
    "uglify-js": "^2.6.1",
    "underscore": "^1.8.3",
    "webpack": "^1.12.9",
    "youtube-node": "^1.3.0"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "^1.0.1",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
